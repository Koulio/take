        final $sourcetype source,
        final DerivationController _derivation) {
        
        _derivation.log($property,DerivationController.BEAN_PROPERTY,source);
        if (source .$accessor () == null || source .$accessor ().size()==0) {
           	$resulttype result = new $resulttype();
